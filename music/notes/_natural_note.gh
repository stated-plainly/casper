import DiatonicNote from @path(parent/_diatonic_note);

import FlatNote from @path(parent/_flat_note);
import SharpNote from @path(parent/_sharp_note);

import NoteLetter from @path(root/shared);
import NoteModifier from @path(root/shared);

export template class NaturalNote : DiatonicNote {
	constructor (letter : NoteLetter) {
		super(letter, []);
	}

	template pub flatten () => FlatNote;

	template pub sharpen () => SharpNote;
}
